{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/hitech/auth/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "account-route": {
        "ClusterId": "account-cluster",
        "Match": {
          "Path": "/api/hitech/accounts/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "post-route": {
        "ClusterId": "post-cluster",
        "Match": {
          "Path": "/api/hitech/posts/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "like-route": {
        "ClusterId": "like-cluster",
        "Match": {
          "Path": "/api/hitech/likes/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "comment-route": {
        "ClusterId": "comment-cluster",
        "Match": {
          "Path": "/api/hitech/comments/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "notification-route": {
        "ClusterId": "notification-cluster",
        "Match": {
          "Path": "/api/hitech/notifications/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "friendship-route": {
        "ClusterId": "friendship-cluster",
        "Match": {
          "Path": "/api/hitech/friendships/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "friend-request-route": {
        "ClusterId": "friend-request-cluster",
        "Match": {
          "Path": "/api/hitech/friend-requests/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "group-route": {
        "ClusterId": "group-cluster",
        "Match": {
          "Path": "/api/hitech/groups/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "join-request-route": {
        "ClusterId": "join-request-cluster",
        "Match": {
          "Path": "/api/hitech/join-requests/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:8001/api/hitech/auth/"
          }
        }
      },
      "account-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:8001/api/hitech/accounts/"
          }
        }
      },
      "post-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:8002/api/hitech/posts/"
          }
        }
      },
      "like-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:8002/api/hitech/likes/"
          }
        }
      },
      "comment-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:8002/api/hitech/comments/"
          }
        }
      },
      "notification-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:8003/api/hitech/notifications/"
          }
        }
      },
      "friendship-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:8004/api/hitech/friendships/"
          }
        }
      },
      "friend-request-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:8004/api/hitech/friend-requests/"
          }
        }
      },
      "group-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:8005/api/hitech/groups/"
          }
        }
      },
      "join-request-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://host.docker.internal:8005/api/hitech/join-requests/"
          }
        }
      }
    }
  }
}
